{
  "name": "bricz-server",
  "version": "0.0.0",
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "init": "yarn install && husky install",
    "lint": "eslint",
    "migrate": "node src/sequelize/migrations/migrate.js",
    "migrate:create": "yarn migrate create",
    "playground": "cross-env ts-node -r dotenv/config scripts/playground.ts",
    "seed:geographicFeatures": "cross-env ts-node -r dotenv/config scripts/geographic-features.ts",
    "start": "node dist/server.js"
  },
  "config": {
    "commitizen": {
      "path": ".yarn/cache/cz-conventional-changelog-npm-3.2.0-0d6f8e98f5-5512b2e28a.zip/node_modules/cz-conventional-changelog",
      "maxHeaderWidth": 72,
      "maxLineWidth": 80
    }
  },
  "dependencies": {
    "@types/geojson": "^7946.0.8",
    "apollo-server-core": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "csv-parse": "^5.0.4",
    "express": "^4.17.1",
    "graphql": "^16.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.intersection": "^4.4.0",
    "lodash.merge": "^4.6.2",
    "multer": "^1.4.4",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.12.0-beta.3",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@graphql-tools/utils": "^8.5.3",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cls-hooked": "^4.3.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/inquirer": "^8.2.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash.intersection": "^4.4.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "JSONStream": "^1.3.5",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "inquirer": "^8.2.0",
    "lint-staged": "^12.1.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "umzug": "^3.0.0"
  }
